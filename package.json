{
  "name": "gulp-vite",
  "type": "module",
  "version": "1.1.0",
  "description": "Use vite as dev server with gulp",
  "main": "index.cjs.js",
  "module": "index.mjs",
  "author": "evromalarkey",
  "license": "MIT",
  "scripts": {
    "build": "rollup index.mjs --file index.cjs.js --format cjs --exports default"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "lodash": "^4.17.21",
    "vite": "^2.3.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "esbuild-plugin-postcss2": "^0.0.9",
    "gulp": "^4.0.2",
    "gulp-esbuild": "^0.8.0",
    "gulp-rename": "^2.0.0",
    "gulp-twig2html": "^2.2.0",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^8.0.1"
  },
  "files": [
    "index.mjs"
  ],
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs.js"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evromalarkey/gulp-vite.git"
  }
}
